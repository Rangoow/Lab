{
  "name": "react-lab",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-lab/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/react-lab"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-lab:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-lab:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "react-lab:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-lab:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-lab:build:development"
        },
        "production": {
          "buildTarget": "react-lab:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/react-lab"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/react-lab"
      },
      "configurations": {
        "ci": {
          "coverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "options": {
        "buildTarget": "react-lab:build"
      }
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/react-lab/**/*.scss"],
        "formatter": "compact"
      }
    }
  },
  "tags": []
}

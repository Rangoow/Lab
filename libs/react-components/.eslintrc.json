{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*",
    "vite.config.js",
    ".storybook"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "@typescript-eslint/no-unsafe-assignment": "off"

      }
    }
  ]
}